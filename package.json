{
  "name": "interview",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run backend:dev\"",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend:dev": "npm run start:dev -w ./backend",
    "backend:build": "npm run build -w ./backend",
    "backend:start": "npm run start -w ./backend",
    "reset-project": "node ./src/scripts/reset-project.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "workspaces": [
    "backend"
  ],
  "dependencies": {
    "@expo/metro-runtime": "~6.1.0",
