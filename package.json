    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "axios": "^1.13.5",
    "expo": "^54.0.1",
    "expo-asset": "~12.0.3",
    "expo-blur": "~15.0.8",
    "expo-calendar": "~15.0.8",
    "expo-constants": "~18.0.3",
