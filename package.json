    "expo-symbols": "~1.0.2",
    "expo-web-browser": "~15.0.2",
    "inversify": "^7.11.0",
    "lucide-react-native": "^0.575.0",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "mobx-react-lite": "^4.1.1",
